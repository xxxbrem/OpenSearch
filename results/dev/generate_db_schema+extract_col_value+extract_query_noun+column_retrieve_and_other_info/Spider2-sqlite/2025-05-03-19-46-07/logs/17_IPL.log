/* Answer the following: Which bowler conceded the highest number of runs in one single over in any match, and in which match did that over occur? */
SELECT p.player_name ,
       x.match_id ,
       x.runs_in_over
FROM   (
          SELECT   d.match_id ,
                   d.over_no ,
                   d.bowler_id ,
                   SUM(d.total_runs) AS runs_in_over
          FROM     deliveries AS d
          GROUP BY d.match_id ,
                   d.over_no ,
                   d.bowler_id
          ORDER BY runs_in_over DESC
          LIMIT    1
       ) AS x
JOIN   players AS p ON p.player_id = x.bowler_id;

/* Answer the following: For each season, which batsman hit the most fours?  (a four is recorded when boundary_type = '4') */
SELECT   m.season ,
         p.player_name ,
         MAX(four_cnt) AS total_fours
FROM     (
            SELECT   d.batsman_id ,
                     m.season ,
                     COUNT(*) AS four_cnt
            FROM     deliveries AS d
            JOIN     matches    AS m ON m.match_id = d.match_id
            WHERE    d.boundary_type = '4'
            GROUP BY d.batsman_id ,
                     m.season
         ) AS t
JOIN     players AS p ON p.player_id = t.batsman_id
GROUP BY m.season
ORDER BY m.season;

/* Answer the following: List the five overs that produced the most total runs across all matches.  Show match id, innings, over number and total runs in that over. */
SELECT d.match_id ,
       d.innings_no ,
       d.over_no ,
       SUM(d.total_runs) AS runs_in_over
FROM   deliveries AS d
GROUP BY d.match_id ,
         d.innings_no ,
         d.over_no
ORDER BY runs_in_over DESC
LIMIT 5;

/* Answer the following: For every match, how many maiden overs (overs that gave away 0 runs) were bowled? */
SELECT  d.match_id ,
        COUNT(*) AS maiden_overs
FROM    (
          SELECT   match_id ,
                   innings_no ,
                   over_no ,
                   SUM(total_runs) AS runs_in_over
          FROM     deliveries
          GROUP BY match_id ,
                   innings_no ,
                   over_no
        ) AS d
WHERE   d.runs_in_over = 0
GROUP BY d.match_id
ORDER BY d.match_id;

/* Answer the following: Which team has the highest average runs scored per over across all matches it has played? */
SELECT   t.team_name ,
         AVG(over_runs) AS avg_runs_per_over
FROM     (
            SELECT   m.team1_id  AS batting_team ,
                     d.match_id ,
                     d.innings_no ,
                     d.over_no ,
                     SUM(d.total_runs) AS over_runs
            FROM     deliveries AS d
            JOIN     matches    AS m ON m.match_id = d.match_id
            GROUP BY d.match_id ,
                     d.innings_no ,
                     d.over_no
         ) AS o
JOIN     teams AS t ON t.team_id = o.batting_team
GROUP BY t.team_id
ORDER BY avg_runs_per_over DESC
LIMIT 1;



